const cssStrings = []
const data = {}
var css
<% cssFiles.forEach((file) => { %>
css = require('<%= file.path %>')
cssStrings.push(css.toString())
data['<%= file.name %>'] = css.locals
<% }) %>

const styleTmpl = require('<%= styleTemplateFile %>')
data.styles = styleTmpl({cssStrings: cssStrings})

const template = require('<%= htmlTemplateFile %>')
const html = template(data)

const fs = require('fs')
fs.writeFileSync('<%= outputFile %>', html, 'utf8')
