const cssStrings = []
const data = {}
var css
<% cssFilenames.forEach((cssFilename) => { %>
css = require('<%= cssFilename.path %>')
cssStrings.push(css.toString())
data['<%= cssFilename.name %>'] = css.locals
<% }) %>

const styleTmpl = require('<%= styleTemplateFilename %>')
data.styles = styleTmpl({cssStrings: cssStrings})

const template = require('<%= htmlTemplateFilename %>')
const html = template(data)

const fs = require('fs')
fs.writeFileSync('<%= outputFilename %>', html, 'utf8')
