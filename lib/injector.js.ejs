const cssStrings = []
const data = {}
var css
<% cssObjs.forEach((cssObj) => { %>
css = require('<%= cssObj.path %>')
cssStrings.push(css.toString())
data['<%= cssObj.name %>'] = css.locals
<% }) %>

const styleTemplate = require('<%= styleTemplatePath %>')
data.styles = styleTemplate({cssStrings: cssStrings})

const htmlTemplate = require('<%= htmlTemplatePath %>')
const html = htmlTemplate(data)

const fs = require('fs')
fs.writeFileSync('<%= outputPath %>', html, 'utf8')
