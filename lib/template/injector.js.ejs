var css
var name

<% cssObjs.forEach((cssObj) => { %>
ns = '<%= cssObj.name %>'
locals = require('<%= cssObj.path %>')
Object.keys(locals).forEach((name) => {
  const selector = ['.', ns, '\\.', name].join('')
  const els = document.querySelectorAll(selector)
  const className = [ns, '.', name].join('')
  const local = locals[name]
  Array.prototype.forEach.call(els, (el) => {
    const cls = el.getAttribute('class').replace(className, local)
    el.setAttribute('class', cls)
  })
})
<% }) %>
